{
  "manifest_version": 2,
  "name": "Ayele",
  "description": "Find and play popular Nigerian sound memes like 'Ayele o' and 'chai' on in an easy-to-use app, built with love by Faffers!",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "icons": { 
    "48": "img/icons/drawable-48-icon.png",
    "128": "img/icons/drawable-48-icon.png" 
  },
  "permissions": [
    "activeTab",
    "https://maxcdn.bootstrapcdn.com/"
  ]
}